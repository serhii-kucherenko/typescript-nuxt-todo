{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Todo List",
  "author": "Kucherenko Sergey",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "start": "nuxt-ts start",
    "eslint": "eslint --ext .ts,.js,.vue . --fix",
    "stylelint": "stylelint --config stylelint.config.js --fix \"**/*.{css,scss,vue}\"",
    "analyze": "nuxt-ts build --analyze"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run eslint",
      "npm run stylelint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.3.3",
    "@nuxtjs/axios": "^5.9.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@types/lodash": "^4.14.149",
    "cross-env": "^6.0.3",
    "element-ui": "^2.13.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "nuxt": "^3.4.3",
    "nuxt-class-component": "^1.3.0",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.11.0"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^0.5.2",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/stylelint-module": "^3.1.1",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.21",
    "@types/swagger-ui-express": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "nuxt-property-decorator": "^2.5.0",
    "postcss-html": "^0.36.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-css-modules": "^2.1.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "ts-node": "^8.5.4",
    "worker-loader": "^2.0.0"
  }
}
